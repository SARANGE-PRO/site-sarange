{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@/*": [
                "./src/*"
            ],
            "@components/*": [
                "./src/components/*"
            ],
            "@features/*": [
                "./src/features/*"
            ],
            "@assets/*": [
                "./src/assets/*"
            ],
            "@utils/*": [
                "./src/utils/*"
            ],
            "@data/*": [
                "./src/data/*"
            ]
        }
    },
    "include": [
        "src"
    ]
}